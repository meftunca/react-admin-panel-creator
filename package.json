{
  "name": "react-admin-panel-creator",
  "version": "0.5.1",
  "private": true,
  "author": "Muhammed Burak Şentürk",
  "license": "MIT",
  "homepage": "http://www.godevloops.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/meftunca/react-admin-panel-creator.git"
  },
  "scripts": {
    "hotel": "clear;hotel run 'nodemon index.js' --port 80",
    "client-install": "npm install --prefix client",
    "start": "node index.js",
    "server": "node index.js",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "heroku-postbuild": "npm run production"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "src/*",
      "node_modules/*"
    ],
    "delay": "2500"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "laravel-mix": "^4.0.15",
    "resolve-url-loader": "3.1.0",
    "sass": "^1.20.1",
    "sass-loader": "7.1.0"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^12.1.0",
    "@ckeditor/ckeditor5-build-inline": "^12.1.0",
    "@ckeditor/ckeditor5-react": "^1.1.3",
    "@date-io/moment": "^1.3.5",
    "@devexpress/dx-react-chart": "^1.10.5",
    "@devexpress/dx-react-chart-material-ui": "^1.10.5",
    "@devexpress/dx-react-core": "^1.10.5",
    "@loadable/component": "^5.9.0",
    "@material-ui/core": "^4.0.0-beta.1",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@material-ui/styles": "^4.0.0-beta.1",
    "axios": "^0.18.0",
    "base64-img": "^1.0.4",
    "bcrypt-nodejs": "^0.0.3",
    "blob-to-base64": "^1.0.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.4",
    "express-session": "^1.16.1",
    "fb": "^2.0.0",
    "fcm-node": "^1.4.1",
    "firebase": "^6.0.2",
    "firebase-admin": "^7.3.0",
    "gmail-api-parse-message": "^2.1.0",
    "googleapis": "^39.2.0",
    "helmet": "^3.18.0",
    "history": "^4.9.0",
    "informed": "^2.6.11",
    "javascript-time-ago": "^2.0.1",
    "material-design-icon-list": "^4.0.1",
    "material-ui-color-picker": "^3.2.0",
    "material-ui-pickers": "^2.2.4",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "moment": "^2.29.2",
    "monk": "^6.0.6",
    "mui-datatables": "^2.1.0",
    "multer": "^1.4.1",
    "node-persist": "^3.0.4",
    "react": "16.8.6",
    "react-content-loader": "^4.2.1",
    "react-dom": "16.8.6",
    "react-dropzone": "^10.1.4",
    "react-email-editor": "^0.9.5",
    "react-facebook-login": "^4.1.1",
    "react-image-lightbox": "^5.1.0",
    "react-loadable": "^5.5.0",
    "react-localize-redux": "^3.5.3",
    "react-router-dom": "^5.0.0",
    "react-spinkit": "^3.0.0",
    "react-swipeable-views": "^0.13.3",
    "react-syntax-highlighter": "^10.2.1",
    "slugify": "^1.3.4",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.2.0",
    "tunnel-agent": "^0.6.0",
    "twit": "^2.2.11",
    "uniqid": "^5.0.3",
    "validate.js": "^0.12.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
}
